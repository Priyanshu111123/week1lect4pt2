#include <iostream>
using namespace std;
int main()
{
  int marks;
  cout<<"Student, please enter the marks to calculate the grade:";
  cin>>marks;
  if(marks>90)
  {
    cout<<'A';
  }
  if(marks<90&&marks>80)
  {
    cout<<'B';
  }
  if(marks<80&&marks>70)
  {
    cout<<'C';
  }
  if(marks<70)
  {
    cout<<'Pass';
  }
}
